<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <layer-list>
            <item android:width="28dp" android:height="28dp" android:drawable="@drawable/switch_foggy_thumb_on" />
        </layer-list>
    </item>
    <item android:state_checked="false">
        <layer-list>
            <item android:width="28dp" android:height="28dp" android:drawable="@drawable/switch_foggy_thumb_off" />
        </layer-list>
    </item>
</selector>
