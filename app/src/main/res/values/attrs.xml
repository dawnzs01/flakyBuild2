<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwitchCardView">
        <!-- The text accompanying the switch. -->
        <attr name="text" format="string" />
        <!-- Whether the switch should be enabled or not. Default true. -->
        <attr name="enabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SuggestedModsAppHeaderView">
        <!-- The app name. -->
        <attr name="app_name" format="string" />
    </declare-styleable>

    <declare-styleable name="ViewStyle">
        <attr name="colorError" format="color" />
        <attr name="colorSurface" format="color" />
        <attr name="colorSecondaryContainer" format="color" />
    </declare-styleable>
</resources>